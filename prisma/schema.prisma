generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pasajero {
  id_usuario String @id(map: "Pasajero_pkey") @default(uuid()) @db.Uuid
  nombre     String
  apellido   String
  telefono   String
  email      String @unique(map: "Pasajero_email_key")
  tarjeta    String?
  id_rol       Int
}
